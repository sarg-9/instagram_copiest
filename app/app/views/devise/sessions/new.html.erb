<%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
  <div class="card float-right w-40 h-45 mt-4">
    <div class="d-flex justify-content-center mt-4">
      <%= image_tag "Logo.png"%>
    </div>
    <div class="d-flex flex-column justify-content-center mt-3">
      <div class="d-flex justify-content-center mt-4 p-0">
        <%= f.email_field :email, autofocus: true, autocomplete: "email",placeholder: "Email" ,class: "w-20"%>
      </div>
      <div class="d-flex justify-content-center mt-4 p-0">
        <%= f.password_field :password, autocomplete: "current-password", placeholder: "Password" ,class: "w-20"%>
      </div>
      <div class="d-flex justify-content-center mt-4 p-0 login_container">
        <%= f.submit "Log in" ,class:"btn btn-primary", class: "w-20" %>
      </div>
      <div class="d-flex justify-content-center links">
        Don't have an account?
      </div>
      <div class="d-flex justify-content-center links">
        <%= render "devise/shared/links" %>
      </div>
    </div>
  </div>
<% end %>

